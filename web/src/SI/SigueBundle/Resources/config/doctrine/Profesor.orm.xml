<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SI\SigueBundle\Entity\Profesor" table="profesor">
    <id name="idprofesor" type="integer" column="idprofesor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="true"/>
    <field name="apellidos" type="string" column="apellidos" length="255" nullable="true"/>
    <field name="dni" type="string" column="dni" length="255" nullable="true"/>
    <field name="correo" type="string" column="correo" length="255" nullable="true"/>
    <field name="password" type="string" column="password" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
